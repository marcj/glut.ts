{
  "name": "@marcj/glut-client",
  "version": "0.0.3",
  "main": "index.ts",
  "author": "Kamille Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --coverage",
    "tsc": "rm -rf dist && tsc",
    "tsc-watch": "rm -rf dist && tsc --watch"
  },
  "peerDependencies": {
    "@marcj/glut-core": "0.0.2",
    "@marcj/marshal": "^0.5.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "6.3.3"
  },
  "dependencies": {
    "@marcj/estdlib": "^0.1.6-",
    "dot-prop": "^4.2.0",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@marcj/glut-core": "^0.0.3",
    "@marcj/marshal": "^0.5.1",
    "@types/dot-prop": "^4.2.0",
    "@types/ws": "^6.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@marcj/glut-*)"
    ],
    "testMatch": [
      "**/tests/**/*.+(ts|tsx|js)"
    ],
    "setupTestFrameworkScriptFile": "jest-extended"
  },
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.run.json"
    }
  }
}

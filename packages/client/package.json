{
  "name": "@kamille/client",
  "version": "0.0.1",
  "main": "dist/main.js",
  "author": "Kamille Team",
  "license": "MIT",
  "bin": "dist/main.js",
  "scripts": {
    "start": "NODE_PRESERVE_SYMLINKS=1 TS_NODE_IGNORE='node_modules/(?!@kamille)' ./node_modules/.bin/ts-node client.ts"
  },
  "peerDependencies": {
  },
  "dependencies": {
    "rxjs": "6.3.3",
    "reflect-metadata": "^0.1.12",
    "@kamille/core": "0.0.1",
    "@types/dot-prop": "^4.2.0",
    "dot-prop": "^4.2.0",
    "@marcj/marshal": "^0.4.8",
    "ws": "^6.1.2",
    "@types/ws": "^6.0.1"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "ts-node": "^7.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@kamille)"
    ],
    "testMatch": [
      "**/tests/**/*.+(ts|tsx|js)"
    ],
    "setupTestFrameworkScriptFile": "jest-extended"
  },
  "globals": {
    "ts-jest": {
      "tsConfigFile": "tsconfig.run.json"
    }
  }
}

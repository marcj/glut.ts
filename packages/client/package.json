{
  "name": "@marcj/glut-client",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Kamille Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --coverage",
    "tsc": "rm -rf dist && tsc"
  },
  "peerDependencies": {
    "rxjs": "6.3.3",
    "reflect-metadata": "^0.1.12",
    "@marcj/glut-core": "0.0.1"
  },
  "dependencies": {
    "@marcj/marshal": "^0.4.8",
    "@marcj/estdlib": "^0.1.1",
    "dot-prop": "^4.2.0",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@marcj/glut-core": "0.0.1",
    "reflect-metadata": "^0.1.13",
    "@types/dot-prop": "^4.2.0",
    "@types/ws": "^6.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@kamille)"
    ],
    "testMatch": [
      "**/tests/**/*.+(ts|tsx|js)"
    ],
    "setupTestFrameworkScriptFile": "jest-extended"
  },
  "globals": {
    "ts-jest": {
      "tsConfigFile": "tsconfig.json"
    }
  }
